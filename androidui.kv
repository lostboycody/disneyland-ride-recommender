##Main screen, prompts park location
<ParkScreen>:
    canvas.before:
        Rectangle:
            pos: self.pos
            size: self.size
            source: 'parkbackground.png'
    BoxLayout:
        orientation: 'vertical'
        spacing: 20
        padding: 400
        size_hint_y: .75

        Label:
            color: 0,0,0,1
            text: "Welcome to the Disneyland Ride Selector!"
            font_size: 50
        Label:
            color: 0,0,0,1
            font_size: 50
            text: "First, start by picking your park"

        Button:
            text: 'Disneyland'
            size_hint: None, None
            size: 1000, 125
            pos_hint: {'center_x' : .5}
            on_press: root.manager.current = 'disneylandareascreen'
            on_press: app.setParkDisneyland()

        Button:
            text: 'Disney California Adventure'
            size_hint: None, None
            size: 1000, 125
            pos_hint: {'center_x' : .5}
            on_press: root.manager.current = 'dcaareascreen'
            on_press: app.setParkDCA()

##Disneyland Area screen, asks area to visit
<DisneylandAreaScreen>:
    canvas.before:
        Rectangle:
            pos: self.pos
            size: self.size
            source: 'areabackground.png'
    BoxLayout:
        orientation: 'vertical'
        spacing: 10
        padding: 60
        size_hint_y: .70

        Label:
            color: 0,0,0,1
            text: "Next, which area would you like to visit?"
            font_size: 50
        Label:
            color: 0,0,0,1
            font_size: 50
            text: "Different lands have different themed rides!"

        Button:
